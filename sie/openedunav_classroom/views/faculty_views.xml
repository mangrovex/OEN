<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="sie_faculty_view_form_inherit" model="ir.ui.view">
            <field name="name">sie.faculty.form.inherit</field>
            <field name="model">sie.faculty</field>
            <field name="inherit_id" ref="openedunav_core.sie_faculty_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='sie_page']" position="after">
                    <page string="Tipo de Docente">
                        <group col="4">
                            <field name="professor"/>
                            <field name="director"/>
                            <field name="planta"/>
                            <field name="division"/>
                            <field name="evaluator"/>
                            <!--<field name="judge"/>-->
                            <!--<field name="statistician"/>-->
                        </group>
                    </page>
                </xpath>
            </field>
        </record>

    </data>
</openerp>