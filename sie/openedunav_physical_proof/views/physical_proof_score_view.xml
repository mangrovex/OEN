<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_sie_physical_proof_score_form" model="ir.ui.view">
            <field name="name">sie.physical.proof.score.form</field>
            <field name="model">sie.physical.proof.score</field>
            <field name="arch" type="xml">
                <form string="Promedio de Pruebas Fisica">
                    <sheet>
                        <group col="6">
                            <field name="param_id" options='{"no_open":True}' readonly="1" colspan="6"/>
                            <field name="measure" readonly="1"/>
                            <field name="value"/>

                            <field name="score"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_sie_physical_proof_score_tree" model="ir.ui.view">
            <field name="name">sie.physical.proof.score.tree</field>
            <field name="model">sie.physical.proof.score</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Physical Proof Score">
                    <field name="param_id" options='{"no_open":True}' readonly="1"/>
                    <field name="measure" readonly="1"/>
                    <field name="value"/>
                    <field name="score"/>
                </tree>
            </field>
        </record>
    </data>
</openerp>